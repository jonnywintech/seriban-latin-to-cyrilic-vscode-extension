{
  "name": "serbian-latin-to-cyrilic-converter",
  "displayName": "Serbian Latin to Cyrillic Converter",
  "description": "Converts Serbian Latin text to Cyrillic",
  "version": "0.0.1",
  "icon": "icon.png",
  "author": "Nikola Stankovic",
  "publisher": "jonnywintech",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonnywintech/seriban-latin-to-cyrilic-vscode-extension.git"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.convertToSerbian",
        "title": "Convert to Serbian Cyrillic"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.18.119",
    "@types/vscode": "^1.95.0",
    "@vscode/vsce": "^3.2.1",
    "typescript": "^5.6.3"
  }
}
